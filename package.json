{
  "name": "authoritarianism-democracy-dashboard",
  "version": "1.0.0",
  "description": "Interactive dashboard analyzing authoritarianism and democracy trends (1945-2025)",
  "main": "src/index.js",
  "scripts": {
    "start": "webpack serve --mode development",
    "build": "webpack --mode production",
    "dev": "webpack serve --mode development --open",
    "analyze-data": "cd scripts && python3 analyze_datasets.py",
    "convert-excel": "cd scripts && python3 convert_excel_to_csv.py",
    "preprocess-vdem": "cd scripts && python3 preprocess_vdem_python.py",
    "setup-data": "cd scripts && ./setup_data.sh",
    "clean-data": "rm -rf dataset/*.json dataset/**/processed_*"
  },
  "dependencies": {
    "d3": "^7.8.5",
    "lodash": "^4.17.21",
    "papaparse": "^5.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "topojson": "^3.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "@babel/preset-react": "^7.22.0",
    "babel-loader": "^9.1.0",
    "css-loader": "^6.8.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "style-loader": "^3.3.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0",
    "webpack-dev-server": "^5.2.2"
  },
  "keywords": [
    "democracy",
    "authoritarianism",
    "data-visualization",
    "dashboard",
    "d3",
    "react"
